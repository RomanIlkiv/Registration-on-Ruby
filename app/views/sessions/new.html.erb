
<div class="container">
  <div class="row">
    <div id="log_in" class="col-lg-6 col-md-6 col-sm-6 col-xs-12 col-lg-offset-3 col-md-offset-3 col-sm-offset-3">
      <%= form_for(:session, url: sessions_path) do |f| %>

        <%= f.label :email %><br>
        <%= f.text_field :email %><br><br><br>

        <%= f.label :password %><br>
        <%= f.password_field :password %><br>
        <%= f.submit "Sign in"%>
      <% end %>
    </div>
  </div>
</div>